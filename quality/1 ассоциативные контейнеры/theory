struct FragmentHasher {
    static const uint64_t N;
    size_t operator() (const Fragment& f) const {
        return static_cast<size_t>(f.begin * N + f.end);
    }
};
–ü–æ –Ω–µ–∫–æ—Ç–æ—Ä—ã–º —Å—Ç–∞—Ç–∏—Å—Ç–∏—á–µ—Å–∫–∏–º —Å–æ–æ–±—Ä–∞–∂–µ–Ω–∏—è–º –æ–±—ã—á–Ω–æ –≤ –∫–∞—á–µ—Å—Ç–≤–µ N –≤—ã–±–∏—Ä–∞—é—Ç –ø—Ä–æ—Å—Ç–æ–µ —á–∏—Å–ª–æ, –Ω–∞–ø—Ä–∏–º–µ—Ä 37. –î–ª—è –æ–±—ã—á–Ω—ã—Ö —Ü–µ–ª–µ–π –±—É–¥–µ—Ç –≤–ø–æ–ª–Ω–µ –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Ç–∞–∫–æ–π —Ö–µ—à-—Ñ—É–Ω–∫—Ü–∏–∏, –Ω–æ –µ—ë –º–∏–Ω—É—Å –≤ —Ç–æ–º, —á—Ç–æ –µ—Å—Ç—å –ø—Ä–æ—Å—Ç–æ–π –∞–ª–≥–æ—Ä–∏—Ç–º, –ø–æ–∑–≤–æ–ª—è—é—â–∏–π –Ω–∞—Ö–æ–¥–∏—Ç—å –∫–æ–ª–ª–∏–∑–∏–∏:
int main() {
    FragmentHasher hasher;
    Fragment f1{10, 1000};
    Fragment f2{10 + 1, 1000 - 37 * 1};
    Fragment f3{10 + 2, 1000 - 37 * 2};
    Fragment f4{10 + 3, 1000 - 37 * 3};

    cout << "f1 hash - "s << hasher(f1) << endl;
    cout << "f2 hash - "s << hasher(f2) << endl;
    cout << "f3 hash - "s << hasher(f3) << endl;
    cout << "f4 hash - "s << hasher(f4) << endl;
}
–†–µ–∑—É–ª—å—Ç–∞—Ç –ø—Ä–æ–≥—Ä–∞–º–º—ã:
