#include <iostream>
#include <string>
#include <deque>

using namespace std;

class TrainTerminal {
public:
    void OnPassengerArrive(const string& name) {
        passengers_on_platform_.push_back(name);
    }

    void OnTrainArrive() {
        cout << "–°–ø–∏—Å–æ–∫ –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤ –ø–æ–µ–∑–¥–∞:"s << endl;
        int id = 1;
        while (!passengers_on_platform_.empty()) {
            cout << (id++) << passengers_on_platform_.front() << endl;
            passengers_on_platform_.pop_front();
        }
    }

    bool FindPassenger(const string& name) const {
        auto iter = find(passengers_on_platform_.begin(), passengers_on_platform_.end(), name);
        return iter != passengers_on_platform_.end();
    }

private:
    deque<std::string> passengers_on_platform_;
};
–≠—Ç–æ—Ç –∫–ª–∞—Å—Å —Ä–µ–∞–ª–∏–∑—É–µ—Ç –∂–µ–ª–µ–∑–Ω–æ–¥–æ—Ä–æ–∂–Ω—É—é –ø–ª–∞—Ç—Ñ–æ—Ä–º—É, –Ω–∞ –∫–æ—Ç–æ—Ä—É—é –ø—Ä–∏–±—ã–≤–∞—é—Ç —ç–ª–µ–∫—Ç—Ä–∏—á–∫–∏ –∏ –ø—Ä–∏—Ö–æ–¥—è—Ç –ø–∞—Å—Å–∞–∂–∏—Ä—ã. –°—á–∏—Ç–∞–µ—Ç—Å—è, —á—Ç–æ –µ—Å—Ç—å —Ç–æ–ª—å–∫–æ –æ–¥–∏–Ω –º–∞—Ä—à—Ä—É—Ç, –ø–æ—ç—Ç–æ–º—É –ø–æ–µ–∑–¥ –∑–∞–±–∏—Ä–∞–µ—Ç –≤—Å–µ—Ö –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤, –∫–æ—Ç–æ—Ä—ã–µ –≤ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç –Ω–∞—Ö–æ–¥—è—Ç—Å—è –Ω–∞ –ø–ª–∞—Ç—Ñ–æ—Ä–º–µ. –ü–æ –ø—Ä–∏–±—ã—Ç–∏–∏ –ø–æ–µ–∑–¥–∞ –ø—Ä–æ–≥—Ä–∞–º–º–∞ –ø–µ—á–∞—Ç–∞–µ—Ç —Å–ø–∏—Å–æ–∫ –≤—Å–µ—Ö –µ–≥–æ –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤. –û–±–æ–∑–Ω–∞—á–∏–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤ —á–µ—Ä–µ–∑ N. –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø–æ–µ–∑–¥–æ–≤ –≤ —ç—Ç–æ—Ç –¥–µ–Ω—å —Å–æ—Å—Ç–∞–≤–ª—è–µ—Ç N/100. –°–ª—É–∂–±–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ 3N —Ä–∞–∑ –∏—Å–∫–∞–ª–∞ –ø–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã—Ö –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤, –ø—Ä–∏–º–µ–Ω—è—è FindPassenger.
–°–ª–æ–∂–Ω–æ—Å—Ç—å –∫–∞–∫–æ–≥–æ –∏–∑ –º–µ—Ç–æ–¥–æ–≤ —ç—Ç–æ–≥–æ –∫–ª–∞—Å—Å–∞ ‚Äî –∞–º–æ—Ä—Ç–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è O(1), –≤ —Ç–æ –≤—Ä–µ–º—è, –∫–∞–∫ —Ö—É–¥—à–∞—è —Å–ª–æ–∂–Ω–æ—Å—Ç—å –±–æ–ª—å—à–µ?
–î–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω–æ, –≤ –Ω–µ—É–¥–∞—á–Ω–æ–º –¥–ª—è –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤ —Å–ª—É—á–∞–µ –º–µ—Ç–æ–¥ OnTrainArrive –æ—Ç–ø—Ä–∞–≤–∏—Ç –∏—Ö –≤—Å–µ—Ö –∏–ª–∏ –ø–æ—á—Ç–∏ –≤—Å–µ—Ö –≤ –æ–¥–Ω–æ–º –ø–æ–µ–∑–¥–µ, –∑–∞—Å—Ç–∞–≤–ª—è—è –ø—Ä–æ–∫–ª–∏–Ω–∞—Ç—å –Ω–µ–Ω–∞–≤–∏—Å—Ç–Ω—ã–π —á–∞—Å –ø–∏–∫ –∏ —Ç—Ä–∞–Ω—Å–ø–æ—Ä—Ç–Ω—É—é –∫–æ–º–ø–∞–Ω–∏—é. –ü–æ—ç—Ç–æ–º—É —Ö—É–¥—à–∞—è —Å–ª–æ–∂–Ω–æ—Å—Ç—å –º–µ—Ç–æ–¥–∞ —Å–æ—Å—Ç–∞–≤–∏—Ç —Ü–µ–ª—ã—Ö O(N). –ù–æ –∫–∞–∂–¥—ã–π –ø–∞—Å—Å–∞–∂–∏—Ä –±—É–¥–µ—Ç –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω –ª–∏—à—å –æ–¥–Ω–∞–∂–¥—ã, –ø–æ—ç—Ç–æ–º—É —Å—É–º–º–∞—Ä–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –≤–∏—Ç–∫–æ–≤ —Ü–∏–∫–ª–∞ –≤–Ω—É—Ç—Ä–∏ OnTrainArrive –Ω–∏–∫–æ–≥–¥–∞ –Ω–µ –ø—Ä–µ–≤—ã—Å–∏—Ç N. –†–∞–∑ N/100 –≤—ã–∑–æ–≤–æ–≤ –º–µ—Ç–æ–¥–∞ –∑–∞—Å—Ç–∞–≤–∏–ª–∏ –µ–≥–æ –æ–±—Ä–∞–±–æ—Ç–∞—Ç—å –≤—Å–µ–≥–æ N –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤, –∑–Ω–∞—á–∏—Ç, –≤ —Å—Ä–µ–¥–Ω–µ–º –æ–¥–∏–Ω –≤—ã–∑–æ–≤ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–ª 100 –ø–∞—Å—Å–∞–∂–∏—Ä–æ–≤, —á—Ç–æ —Ç–æ–∂–µ –≤–ø–∏—Å—ã–≤–∞–µ—Ç—Å—è –≤ –æ–±–æ–∑–Ω–∞—á–µ–Ω–∏–µ O(1).

–≠—Ç–æ –µ—â—ë –æ–¥–∏–Ω –∑–∞–º–µ—á–∞—Ç–µ–ª—å–Ω—ã–π –ø—Ä–∏–º–µ—Ä –∞–º–æ—Ä—Ç–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–æ–π —Å–ª–æ–∂–Ω–æ—Å—Ç–∏: –∫–∞–∂–¥—ã–π –≤—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ –º–æ–∂–µ—Ç –æ–±—Ä–∞–±–æ—Ç–∞—Ç—å –º–Ω–æ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–æ–≤, –Ω–æ –≤—Å–µ –≤—ã–∑–æ–≤—ã –≤ —Å–æ–≤–æ–∫—É–ø–Ω–æ—Å—Ç–∏ –æ–±—Ä–∞–±–æ—Ç–∞—é—Ç –Ω–µ –±–æ–ª—å—à–µ –æ–±—â–µ–≥–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ —ç–ª–µ–º–µ–Ω—Ç–æ–≤.
–ù–µ –ø—É—Ç–∞–π—Ç–µ –∞–º–æ—Ä—Ç–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω—É—é —Å–ª–æ–∂–Ω–æ—Å—Ç—å —Å–æ —Å—Ä–µ–¥–Ω–µ–π. –ù–µ–∫–æ—Ç–æ—Ä—ã–µ –∞–ª–≥–æ—Ä–∏—Ç–º—ã –º–æ–≥—É—Ç —Ä–∞–±–æ—Ç–∞—Ç—å –±—ã—Å—Ç—Ä–æ –∏–ª–∏ –º–µ–¥–ª–µ–Ω–Ω–æ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –≤—Ö–æ–¥–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö. –ù–∞–ø—Ä–∏–º–µ—Ä, —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞. –û–±—ã—á–Ω–æ –æ–Ω–∞ —Ä–∞–±–æ—Ç–∞–µ—Ç –∑–∞ O(NlogN), –Ω–æ –µ—Å–ª–∏ –≤–µ–∫—Ç–æ—Ä –æ—Ç—Å–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω –∏–∑–Ω–∞—á–∞–ª—å–Ω–æ, —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –º–æ–∂–µ—Ç –æ–ø—Ä–µ–¥–µ–ª–∏—Ç—å —ç—Ç–æ –∑–∞ O(N) –∏ –∑–∞–≤–µ—Ä—à–∏—Ç—å—Å—è. –í —ç—Ç–æ–º –ª—É—á—à–µ–º —Å–ª—É—á–∞–µ –µ—ë —Å–ª–æ–∂–Ω–æ—Å—Ç—å –±—É–¥–µ—Ç O(N). –°—Ä–µ–¥–Ω–µ–π —Å–ª–æ–∂–Ω–æ—Å—Ç—å—é –Ω–∞–∑—ã–≤–∞—é—Ç —É—Å—Ä–µ–¥–Ω–µ–Ω–∏–µ —Å–ª–æ–∂–Ω–æ—Å—Ç–∏ –ø–æ –≤—Å–µ–º –≤–æ–∑–º–æ–∂–Ω—ã–º –≤—Ö–æ–¥–Ω—ã–º –¥–∞–Ω–Ω—ã–º. –≠—Ç–æ –≤–æ–≤—Å–µ –Ω–µ –≥–∞—Ä–∞–Ω—Ç–∏—Ä—É–µ—Ç –±—ã—Å—Ç—Ä—É—é —Ä–∞–±–æ—Ç—É –≤ –∫–∞–∂–¥–æ–º —Å–ª—É—á–∞–µ. –ù–∞–ø—Ä–∏–º–µ—Ä, –∑–ª–æ—É–º—ã—à–ª–µ–Ω–Ω–∏–∫–∏ –º–æ–≥—É—Ç —Å–ø–µ—Ü–∏–∞–ª—å–Ω–æ –ø–æ–¥–æ–±—Ä–∞—Ç—å –¥–∞–Ω–Ω—ã–µ, —á—Ç–æ–±—ã ¬´–ø–æ–≤–µ—Å–∏—Ç—å¬ª –∞–ª–≥–æ—Ä–∏—Ç–º, –¥–∞–∂–µ –µ—Å–ª–∏ –æ–Ω –∏–º–µ–µ—Ç —Ö–æ—Ä–æ—à—É—é —Å—Ä–µ–¥–Ω—é—é —Å–ª–æ–∂–Ω–æ—Å—Ç—å.
–í —Ç–æ –∂–µ –≤—Ä–µ–º—è –∞–º–æ—Ä—Ç–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è —Å–ª–æ–∂–Ω–æ—Å—Ç—å –≥–∞—Ä–∞–Ω—Ç–∏—Ä—É–µ—Ç –±—ã—Å—Ç—Ä—É—é —Ä–∞–±–æ—Ç—É ‚Äî –≤–µ—Ä–Ω–µ–µ, –±—ã—Å—Ç—Ä—É—é —Ä–∞–±–æ—Ç—É –ø—Ä–æ–≥—Ä–∞–º–º—ã –≤ –∏—Ç–æ–≥–µ, –ø–æ—Å–ª–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è –º–Ω–æ–≥–∏—Ö –ø–æ–¥–æ–±–Ω—ã—Ö –æ–ø–µ—Ä–∞—Ü–∏–π. –ê–º–æ—Ä—Ç–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è —Å–ª–æ–∂–Ω–æ—Å—Ç—å –Ω–µ –≥–∞—Ä–∞–Ω—Ç–∏—Ä—É–µ—Ç, —á—Ç–æ –∫–∞–∂–¥—ã–π –≤—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ –∏–ª–∏ –º–µ—Ç–æ–¥–∞ –±—É–¥–µ—Ç –±—ã—Å—Ç—Ä—ã–º. –ù–æ –µ—Å–ª–∏ —Ç–∞–∫–∏—Ö –≤—ã–∑–æ–≤–æ–≤ —Å–¥–µ–ª–∞—Ç—å –º–Ω–æ–≥–æ, –ø–ª–æ—Ö–∏—Ö —Å–ª—É—á–∞–µ–≤ –æ–∫–∞–∂–µ—Ç—Å—è –º–∞–ª–æ, –∏ –≤ —Å–æ–≤–æ–∫—É–ø–Ω–æ—Å—Ç–∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –æ–ø–µ—Ä–∞—Ü–∏–π –±—É–¥–µ—Ç –≤—Å—ë —Ä–∞–≤–Ω–æ –Ω–µ–±–æ–ª—å—à–∏–º.
–ù–µ —Ö—É–¥—à–∏–π —Å–ª—É—á–∞–π
–ß—Ç–æ–±—ã –æ—Ü–µ–Ω–∏—Ç—å, —Å–∫–æ–ª—å–∫–æ –ø—Ä–∏–º–µ—Ä–Ω–æ –≤—Ä–µ–º–µ–Ω–∏ –±—É–¥–µ—Ç —Ä–∞–±–æ—Ç–∞—Ç—å –≤–∞—à –∞–ª–≥–æ—Ä–∏—Ç–º, –æ—Ä–∏–µ–Ω—Ç–∏—Ä—É–π—Ç–µ—Å—å –Ω–∞ –ø—Ä–∞–≤–∏–ª–æ: —è–¥—Ä–æ –ø—Ä–æ—Ü–µ—Å—Å–æ—Ä–∞ –≤—ã–ø–æ–ª–Ω—è–µ—Ç –æ–∫–æ–ª–æ –º–∏–ª–ª–∏–∞—Ä–¥–∞ –¥–µ–π—Å—Ç–≤–∏–π –≤ —Å–µ–∫—É–Ω–¥—É. –û—Ü–µ–Ω–∫–∞ –≥—Ä—É–±–∞—è, –Ω–æ –≤ –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö —Å–ª—É—á–∞—è—Ö –æ–Ω–∞ –ø–æ–ª–µ–∑–Ω–∞. –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –¥–µ–π—Å—Ç–≤–∏–π –º–æ–∂–Ω–æ –ø–æ–¥—Å—á–∏—Ç–∞—Ç—å —Ç–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–∏. –†–∞–∑–±–µ—Ä—ë–º—Å—è, –≤—Å–µ–≥–¥–∞ –ª–∏ —Ç–µ–æ—Ä–∏—è –≤–µ—Ä–Ω–∞.
–í –Ω–∞—á–∞–ª–µ —Ç–µ–º—ã –º—ã —Ä–∞—Å—Å–º–æ—Ç—Ä–µ–ª–∏ —Ñ—É–Ω–∫—Ü–∏—é, –∫–æ—Ç–æ—Ä–∞—è –æ–ø—Ä–µ–¥–µ–ª—è–µ—Ç, –æ–¥–∏–Ω–∞–∫–æ–≤—ã–µ –ª–∏ —ç–ª–µ–º–µ–Ω—Ç—ã –≤ –¥–≤—É—Ö –≤–µ–∫—Ç–æ—Ä–∞—Ö. –í—Å–ø–æ–º–Ω–∏–º –∫–æ–¥:
// O(n¬≤)
bool TestPermut(const vector<int>& v1, const vector<int>& v2) {
    // –µ—Å–ª–∏ –æ–Ω–∏ —Ä–∞–∑–Ω–æ–π –¥–ª–∏–Ω—ã, —ç–ª–µ–º–µ–Ω—Ç—ã –∑–∞–≤–µ–¥–æ–º–æ —Ä–∞–∑–Ω—ã–µ
    if (v1.size() != v2.size()) {
        return false;
    }

    for (int i : v1) {
        // –ø—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –∫–∞–∂–¥—ã–π —ç–ª–µ–º–µ–Ω—Ç –ø–µ—Ä–≤–æ–≥–æ –≤–µ–∫—Ç–æ—Ä–∞
        // —Å–æ–¥–µ—Ä–∂–∏—Ç—Å—è –æ–¥–∏–Ω–∞–∫–æ–≤–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ä–∞–∑ –≤ –æ–±–æ–∏—Ö –≤–µ–∫—Ç–æ—Ä–∞—Ö
        if (count(v1.begin(), v1.end(), i) != count(v2.begin(), v2.end(), i)) {
            return false;
        }
    }

    return true;
}

// O(n log n)
bool TestPermut2(const vector<int>& v1, const vector<int>& v2) {
    auto v1_copy = v1;
    auto v2_copy = v2;

    std::sort(v1_copy.begin(), v1_copy.end());
    std::sort(v2_copy.begin(), v2_copy.end());

    return v1_copy == v2_copy;
}
–í–æ–∑–º–æ–∂–Ω–æ, –≤—ã –æ–±—Ä–∞—Ç–∏–ª–∏ –≤–Ω–∏–º–∞–Ω–∏–µ –Ω–∞ —Å—Ç—Ä–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç: –Ω–∞–∏–≤–Ω—ã–π –∫–≤–∞–¥—Ä–∞—Ç–∏—á–Ω—ã–π –∞–ª–≥–æ—Ä–∏—Ç–º –∏–∑ —É—Å–ª–æ–≤–∏—è –±—ã—Å—Ç—Ä–æ –∑–∞–≤–µ—Ä—à–∞–ª—Å—è –ø—Ä–∏ –ø–µ—Ä–≤–æ–º –∑–∞–ø—É—Å–∫–µ, –Ω–æ –¥–ª—è –æ—Ç–≤–µ—Ç–∞ true –µ–º—É –Ω—É–∂–Ω–æ –±—ã–ª–æ –º–Ω–æ–≥–æ –≤—Ä–µ–º–µ–Ω–∏. –ê –≤—Ç–æ—Ä–æ–π –∞–ª–≥–æ—Ä–∏—Ç–º –≤—Å–µ–≥–¥–∞ —Ä–∞–±–æ—Ç–∞–µ—Ç –ø—Ä–∏–º–µ—Ä–Ω–æ –æ–¥–∏–Ω–∞–∫–æ–≤–æ –±—ã—Å—Ç—Ä–æ. –°—Ä–∞–≤–Ω–∏–º –∞–ª–≥–æ—Ä–∏—Ç–º—ã –Ω–∞ —Å–∏–ª—å–Ω–æ —Ä–∞–∑–ª–∏—á–∞—é—â–∏—Ö—Å—è –≤–µ–∫—Ç–æ—Ä–∞—Ö –∏–∑ 500 000 —ç–ª–µ–º–µ–Ω—Ç–æ–≤:
O(n¬≤): 2 ms
O(n log n): 181 ms
–ú–æ–∂–µ—Ç –ø–æ–∫–∞–∑–∞—Ç—å—Å—è, —á—Ç–æ —Ç–µ–æ—Ä–∏—è –æ–±–º–∞–Ω—ã–≤–∞–µ—Ç. –°–¥–µ–ª–∞–µ–º –≤–µ–∫—Ç–æ—Ä—ã –ø–æ—Ö–æ–∂–∏–º–∏:
int main() {
    std::mt19937 g;

    // ...

    v2 = v1;
    shuffle(v2.begin(), v2.end(), g);
    v2[rand() % v2.size()]++;

    // ...
}
–†–∞–∑–ª–∏—á–∏–µ —Ç–æ–ª—å–∫–æ –≤ –æ–¥–Ω–æ–º —ç–ª–µ–º–µ–Ω—Ç–µ —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º –∏–Ω–¥–µ–∫—Å–æ–º. –†–µ–∑—É–ª—å—Ç–∞—Ç—ã —Ä–∞–±–æ—Ç—ã:
O(n¬≤): 2286 ms
O(n log n): 178 ms
–ù–∞ —ç—Ç–æ—Ç —Ä–∞–∑ –≤—Å—ë –∫–∞–∫ –∏ –¥–æ–ª–∂–Ω–æ –±—ã—Ç—å.
–ü–æ—á–µ–º—É —Ç–∞–∫ –ø—Ä–æ–∏—Å—Ö–æ–¥–∏—Ç?
–ï—Å–ª–∏ –≤–µ–∫—Ç–æ—Ä—ã —Å—É—â–µ—Å—Ç–≤–µ–Ω–Ω–æ –æ—Ç–ª–∏—á–∞—é—Ç—Å—è, –ø–µ—Ä–≤—ã–π –∞–ª–≥–æ—Ä–∏—Ç–º –±—ã—Å—Ç—Ä–æ –Ω–∞–π–¥—ë—Ç —ç–ª–µ–º–µ–Ω—Ç, –∫–æ—Ç–æ—Ä—ã–π –µ—Å—Ç—å –≤ –ø–µ—Ä–≤–æ–º –≤–µ–∫—Ç–æ—Ä–µ, –Ω–æ –æ—Ç—Å—É—Ç—Å—Ç–≤—É–µ—Ç –≤–æ –≤—Ç–æ—Ä–æ–º. –ü–æ—Å–ª–µ —ç—Ç–æ–≥–æ –∞–ª–≥–æ—Ä–∏—Ç–º —Å—Ä–∞–∑—É –∑–∞–≤–µ—Ä—à–∏—Ç—Å—è. –ù–æ –¥–ª—è –æ—Ç–≤–µ—Ç–∞ true –µ–º—É –ø–æ–Ω–∞–¥–æ–±–∏—Ç—Å—è –º–Ω–æ–≥–æ –≤—Ä–µ–º–µ–Ω–∏. –ê –≤—Ç–æ—Ä–æ–π, –±—ã—Å—Ç—Ä—ã–π –∞–ª–≥–æ—Ä–∏—Ç–º –±—É–¥–µ—Ç –≤ –ª—é–±–æ–º —Å–ª—É—á–∞–µ –≤—ã–ø–æ–ª–Ω—è—Ç—å –¥–≤–µ —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∏ ‚Äî –≤ —ç—Ç–æ–º —Å–º—ã—Å–ª–µ –æ–Ω –±–æ–ª–µ–µ —Å—Ç–∞–±–∏–ª—å–Ω—ã–π.

–ü–æ–¥–æ–±–Ω—ã–π —ç—Ñ—Ñ–µ–∫—Ç –º–æ–∂–Ω–æ –Ω–∞–±–ª—é–¥–∞—Ç—å –∏ —Å upper_bound. –í —É—Ä–æ–∫–µ –æ –ª–æ–≥–∞—Ä–∏—Ñ–º–µ –≤—ã —É–≤–∏–¥–µ–ª–∏, —á—Ç–æ —ç—Ç–æ—Ç –∞–ª–≥–æ—Ä–∏—Ç–º —Å–∏–ª—å–Ω–æ –≤—ã–∏–≥—Ä—ã–≤–∞–µ—Ç —É find_if. –ü–æ—Å–º–æ—Ç—Ä–∏–º, –≤—Å–µ–≥–¥–∞ –ª–∏. –í–µ—Ä–Ω—ë–º—Å—è –∫ —Ç–æ–º—É –ø—Ä–∏–º–µ—Ä—É, –Ω–æ —Ç–µ–ø–µ—Ä—å –≤–º–µ—Å—Ç–æ 500 000 000 –±—É–¥–µ–º –∏—Å–∫–∞—Ç—å —á–∏—Å–ª–æ, –ø—Ä–µ–≤—ã—à–∞—é—â–µ–µ 100:
...
int result_number;
{
    LOG_DURATION("std::upper_bound"s);
    for (int i = 0; i < SEARCHES; ++i) {
        auto iter = upper_bound(nums.begin(), nums.end(), 100);
        result_number = *iter;
    }
}
cout << result_number << endl;

{
    LOG_DURATION("std::find_if"s);
    for (int i = 0; i < SEARCHES; ++i) {
        auto iter = find_if(nums.begin(), nums.end(), [](int x) {
            return x > 100;
        });
        result_number = *iter;
    }
}
cout << result_number << endl;
...
–†–µ–∑—É–ª—å—Ç–∞—Ç –º–æ–∂–µ—Ç –±—ã—Ç—å —Ç–∞–∫–∏–º:
std::upper_bound: 58 ms
352
std::find_if: 0 ms
352
–û–±–∞ –∞–ª–≥–æ—Ä–∏—Ç–º–∞ –≤—ã–¥–∞–ª–∏ –æ–¥–∏–Ω–∞–∫–æ–≤—ã–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç, –Ω–æ find_if —Å–¥–µ–ª–∞–ª —ç—Ç–æ –≥–æ—Ä–∞–∑–¥–æ –±—ã—Å—Ç—Ä–µ–µ –≤–æ–ø—Ä–µ–∫–∏ —Ç–µ–æ—Ä–∏–∏.
–ö–∞–∫ –≤—ã –¥—É–º–∞–µ—Ç–µ, –ø–æ—á–µ–º—É –≤ —ç—Ç–æ–º —Å–ª—É—á–∞–µ find_if –æ–∫–∞–∑–∞–ª—Å—è –±—ã—Å—Ç—Ä–µ–µ, –Ω–µ—Å–º–æ—Ç—Ä—è –Ω–∞ —Ç–æ, —á—Ç–æ –µ–≥–æ —Å–ª–æ–∂–Ω–æ—Å—Ç—å –±–æ–ª—å—à–µ?
–°–ª–æ–∂–Ω–æ—Å—Ç—å find_if ‚Äî —ç—Ç–æ O(N) –≤ —Ö—É–¥—à–µ–º —Å–ª—É—á–∞–µ. –ù–æ —Ç—É—Ç –ø–æ–≤–µ–∑–ª–æ: —Å–ª—É—á–∞–π –æ–∫–∞–∑–∞–ª—Å—è –¥–∞–ª–µ–∫–æ –Ω–µ —Å–∞–º—ã–º —Ö—É–¥—à–∏–º –¥–ª—è —ç—Ç–æ–≥–æ –∞–ª–≥–æ—Ä–∏—Ç–º–∞.
–ü—Ä–æ—Ö–æ–¥—è –≤–µ–∫—Ç–æ—Ä, find_if –±—ã—Å—Ç—Ä–æ –Ω–∞—Ö–æ–¥–∏—Ç –Ω—É–∂–Ω–æ–µ —á–∏—Å–ª–æ, –≤ —Ç–æ –≤—Ä–µ–º—è –∫–∞–∫ upper_bound –≤ –ª—é–±–æ–º —Å–ª—É—á–∞–µ –±—É–¥–µ—Ç —Å—Ö–æ–¥–∏—Ç—å—Å—è –∫ –Ω–µ–º—É –±–∏–Ω–∞—Ä–Ω—ã–º –ø–æ–∏—Å–∫–æ–º. –û–¥–Ω–∞–∫–æ —ç—Ç–æ –Ω–µ –∑–Ω–∞—á–∏—Ç, —á—Ç–æ –Ω—É–∂–Ω–æ –≤—Å–µ–≥–¥–∞ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å find_if ‚Äî –≤ —Å—Ä–µ–¥–Ω–µ–º upper_bound –≥–æ—Ä–∞–∑–¥–æ –±—ã—Å—Ç—Ä–µ–µ.

## –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–π –º–∞—Ç–µ—Ä–∏–∞–ª –¥–ª—è –ª—É—á—à–µ–≥–æ –ø–æ–Ω–∏–º–∞–Ω–∏—è

### –ü—Ä–∞–∫—Ç–∏—á–µ—Å–∫–∏–µ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ –ø–æ –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏ –∫–æ–¥–∞ (C++17)

#### Memory Management –∏ Cache Efficiency

**–•–û–†–û–®–û:**
```cpp
// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::vector –¥–ª—è –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–æ–≤ - –¥–∞–Ω–Ω—ã–µ –ª–µ–∂–∞—Ç –Ω–µ–ø—Ä–µ—Ä—ã–≤–Ω–æ
std::vector<int> data;
data.reserve(1000); // –†–µ–∑–µ—Ä–≤–∏—Ä—É–µ–º –ø–∞–º—è—Ç—å –∑–∞—Ä–∞–Ω–µ–µ

// –ü—Ä–æ—Ö–æ–¥–∏–º –ø–æ –¥–∞–Ω–Ω—ã–º –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ (cache-friendly)
for (const auto& item : data) {
    process(item);
}

// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::array –¥–ª—è —Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö —Ä–∞–∑–º–µ—Ä–æ–≤
std::array<int, 100> fixed_data;
```

**–ü–õ–û–•–û:**
```cpp
// std::list - –¥–∞–Ω–Ω—ã–µ —Ä–∞–∑–±—Ä–æ—Å–∞–Ω—ã –ø–æ –ø–∞–º—è—Ç–∏
std::list<int> data;

// –°–ª—É—á–∞–π–Ω—ã–π –¥–æ—Å—Ç—É–ø –∫ —ç–ª–µ–º–µ–Ω—Ç–∞–º (cache-unfriendly)
for (int i = 0; i < data.size(); i += rand() % 10) {
    process(data[i]);
}

// –î–∏–Ω–∞–º–∏—á–µ—Å–∫–æ–µ –≤—ã–¥–µ–ª–µ–Ω–∏–µ –±–µ–∑ reserve
std::vector<int> vec;
for (int i = 0; i < 1000000; ++i) {
    vec.push_back(i); // –ú–Ω–æ–∂–µ—Å—Ç–≤–µ–Ω–Ω—ã–µ —Ä–µ–∞–ª–ª–æ–∫–∞—Ü–∏–∏
}
```

#### –í—ã–±–æ—Ä –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–æ–≤ –ø–æ —Å–ª–æ–∂–Ω–æ—Å—Ç–∏ –æ–ø–µ—Ä–∞—Ü–∏–π

**–•–û–†–û–®–û:**
```cpp
// –î–ª—è —á–∞—Å—Ç—ã—Ö –ø–æ–∏—Å–∫–æ–≤ - –∏—Å–ø–æ–ª—å–∑—É–µ–º std::unordered_map O(1)
std::unordered_map<std::string, int> fast_lookup;

// –î–ª—è —Å–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö —Å –ø–æ–∏—Å–∫–æ–º - std::set O(log n)
std::set<int> sorted_data;

// –î–ª—è —Å—Ç–µ–∫–∞/–æ—á–µ—Ä–µ–¥–∏ - std::deque O(1) –¥–ª—è –æ–±–µ–∏—Ö —Å—Ç–æ—Ä–æ–Ω
std::deque<int> queue_data;

// –î–ª—è —á–∞—Å—Ç—ã—Ö –≤—Å—Ç–∞–≤–æ–∫ –≤ –ø—Ä–æ–∏–∑–≤–æ–ª—å–Ω–æ–µ –º–µ—Å—Ç–æ - std::list O(1)
std::list<int> insertion_heavy;
```

**–ü–õ–û–•–û:**
```cpp
// –ü–æ–∏—Å–∫ –≤ std::vector O(n) –¥–ª—è –∫–∞–∂–¥–æ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–∞
std::vector<std::pair<std::string, int>> slow_lookup;
auto it = std::find_if(slow_lookup.begin(), slow_lookup.end(), 
    [&](const auto& p) { return p.first == key; });

// –ß–∞—Å—Ç—ã–µ –≤—Å—Ç–∞–≤–∫–∏ –≤ –Ω–∞—á–∞–ª–æ std::vector O(n)
std::vector<int> vec;
vec.insert(vec.begin(), new_element);

// –ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ std::map –∫–æ–≥–¥–∞ –ø–æ—Ä—è–¥–æ–∫ –Ω–µ –≤–∞–∂–µ–Ω
std::map<std::string, int> ordered_when_not_needed;
```

#### –ê–ª–≥–æ—Ä–∏—Ç–º—ã –∏ –∏—Ç–µ—Ä–∞—Ç–æ—Ä—ã

**–•–û–†–û–®–û:**
```cpp
// –ò—Å–ø–æ–ª—å–∑—É–µ–º –∞–ª–≥–æ—Ä–∏—Ç–º—ã STL –≤–º–µ—Å—Ç–æ —Ä—É—á–Ω—ã—Ö —Ü–∏–∫–ª–æ–≤
std::sort(vec.begin(), vec.end());
auto it = std::lower_bound(vec.begin(), vec.end(), value);

// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::transform –¥–ª—è –ø—Ä–µ–æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏–π
std::transform(input.begin(), input.end(), output.begin(), 
    [](int x) { return x * 2; });

// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::accumulate –¥–ª—è –∞–≥—Ä–µ–≥–∞—Ü–∏–∏
int sum = std::accumulate(vec.begin(), vec.end(), 0);

// –ò—Å–ø–æ–ª—å–∑—É–µ–º range-based for –∫–æ–≥–¥–∞ –≤–æ–∑–º–æ–∂–Ω–æ
for (const auto& item : container) {
    process(item);
}
```

**–ü–õ–û–•–û:**
```cpp
// –†—É—á–Ω–∞—è —Ä–µ–∞–ª–∏–∑–∞—Ü–∏—è —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∏
for (int i = 0; i < vec.size(); ++i) {
    for (int j = i + 1; j < vec.size(); ++j) {
        if (vec[i] > vec[j]) {
            std::swap(vec[i], vec[j]);
        }
    }
}

// –†—É—á–Ω–æ–π –ø–æ–∏—Å–∫ –≤ –æ—Ç—Å–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω–Ω–æ–º –º–∞—Å—Å–∏–≤–µ
bool found = false;
for (int i = 0; i < vec.size(); ++i) {
    if (vec[i] == value) {
        found = true;
        break;
    }
}
```

#### –°—Ç—Ä–æ–∫–∏ –∏ –∏—Ö –æ–±—Ä–∞–±–æ—Ç–∫–∞

**–•–û–†–û–®–û:**
```cpp
// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::string_view –¥–ª—è read-only –æ–ø–µ—Ä–∞—Ü–∏–π (C++17)
void process_string(std::string_view str) {
    // –ù–µ—Ç –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è —Å—Ç—Ä–æ–∫–∏
}

// –†–µ–∑–µ—Ä–≤–∏—Ä—É–µ–º –ø–∞–º—è—Ç—å –¥–ª—è —Å—Ç—Ä–æ–∫
std::string result;
result.reserve(estimated_size);

// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::ostringstream –¥–ª—è –∫–æ–Ω–∫–∞—Ç–µ–Ω–∞—Ü–∏–∏
std::ostringstream oss;
oss << "Hello " << name << "!";
std::string result = oss.str();
```

**–ü–õ–û–•–û:**
```cpp
// –ü–µ—Ä–µ–¥–∞—á–∞ —Å—Ç—Ä–æ–∫ –ø–æ –∑–Ω–∞—á–µ–Ω–∏—é
void process_string(std::string str) {
    // –ù–µ–Ω—É–∂–Ω–æ–µ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ
}

// –ö–æ–Ω–∫–∞—Ç–µ–Ω–∞—Ü–∏—è —Å—Ç—Ä–æ–∫ —á–µ—Ä–µ–∑ operator+
std::string result = "";
for (const auto& item : items) {
    result = result + item + " "; // O(n¬≤) —Å–ª–æ–∂–Ω–æ—Å—Ç—å
}
```

#### Move —Å–µ–º–∞–Ω—Ç–∏–∫–∞ –∏ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ (C++11+)

**–•–û–†–û–®–û:**
```cpp
// –ò—Å–ø–æ–ª—å–∑—É–µ–º std::move –¥–ª—è –¥–æ—Ä–æ–≥–∏—Ö –æ–±—ä–µ–∫—Ç–æ–≤
std::vector<std::string> vec;
vec.push_back(std::move(expensive_string));

// –í–æ–∑–≤—Ä–∞—â–∞–µ–º –ø–æ –∑–Ω–∞—á–µ–Ω–∏—é - RVO —Å—Ä–∞–±–æ—Ç–∞–µ—Ç
std::vector<int> create_vector() {
    std::vector<int> result;
    // ... –∑–∞–ø–æ–ª–Ω—è–µ–º result
    return result; // –ù–µ—Ç –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è –±–ª–∞–≥–æ–¥–∞—Ä—è RVO
}

// –ò—Å–ø–æ–ª—å–∑—É–µ–º emplace –≤–º–µ—Å—Ç–æ push
vec.emplace_back(arg1, arg2, arg3); // –ö–æ–Ω—Å—Ç—Ä—É–∏—Ä—É–µ—Ç –Ω–∞ –º–µ—Å—Ç–µ
```

**–ü–õ–û–•–û:**
```cpp
// –ù–µ–Ω—É–∂–Ω–æ–µ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ
vec.push_back(expensive_string); // –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ –≤–º–µ—Å—Ç–æ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è

// –í–æ–∑–≤—Ä–∞—Ç —á–µ—Ä–µ–∑ –ø–∞—Ä–∞–º–µ—Ç—Ä
void create_vector(std::vector<int>& result) {
    // ... –∑–∞–ø–æ–ª–Ω—è–µ–º result
}

// –°–æ–∑–¥–∞–Ω–∏–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–≥–æ –æ–±—ä–µ–∫—Ç–∞
vec.push_back(MyClass(arg1, arg2, arg3)); // –°–æ–∑–¥–∞–Ω–∏–µ + –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ
```

#### –ò–∑–±–µ–≥–∞–Ω–∏–µ –ø—Ä–µ–∂–¥–µ–≤—Ä–µ–º–µ–Ω–Ω–æ–π –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏

**–•–û–†–û–®–û:**
```cpp
// –°–Ω–∞—á–∞–ª–∞ –ø–∏—à–µ–º —á–∏—Ç–∞–µ–º—ã–π –∫–æ–¥
auto result = std::find_if(data.begin(), data.end(), predicate);

// –ò–∑–º–µ—Ä—è–µ–º –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å
auto start = std::chrono::high_resolution_clock::now();
// ... –∫–æ–¥ –¥–ª—è –∏–∑–º–µ—Ä–µ–Ω–∏—è
auto end = std::chrono::high_resolution_clock::now();
auto duration = std::chrono::duration_cast<std::chrono::microseconds>(end - start);

// –û–ø—Ç–∏–º–∏–∑–∏—Ä—É–µ–º —Ç–æ–ª—å–∫–æ —É–∑–∫–∏–µ –º–µ—Å—Ç–∞
if (is_performance_critical && profiler_shows_bottleneck) {
    // –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–Ω–∞—è –≤–µ—Ä—Å–∏—è
}
```

**–ü–õ–û–•–û:**
```cpp
// –ü—Ä–µ–∂–¥–µ–≤—Ä–µ–º–µ–Ω–Ω–∞—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –±–µ–∑ –∏–∑–º–µ—Ä–µ–Ω–∏–π
// –°–ª–æ–∂–Ω—ã–π –∫–æ–¥ –±–µ–∑ –¥–æ–∫–∞–∑–∞—Ç–µ–ª—å—Å—Ç–≤–∞ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏
inline __attribute__((always_inline)) void complex_optimization() {
    // –ö–æ–¥, –∫–æ—Ç–æ—Ä—ã–π –º–æ–∂–µ—Ç –±—ã—Ç—å –∏ –Ω–µ –Ω—É–∂–µ–Ω
}

// –ú–∏–∫—Ä–æ–æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏ –≤ —É—â–µ—Ä–± —á–∏—Ç–∞–µ–º–æ—Å—Ç–∏
int result = condition ? (expensive_calc(), value1) : value2;
```

### –°–æ–≤—Ä–µ–º–µ–Ω–Ω—ã–µ –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–∏ C++17 –¥–ª—è –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏

#### Structured bindings –¥–ª—è —É–¥–æ–±—Å—Ç–≤–∞

**–•–û–†–û–®–û:**
```cpp
// Structured bindings (C++17)
auto [key, value] = map.insert({k, v});
if (!key->second) {
    // –û–±—Ä–∞–±–æ—Ç–∫–∞
}

// –ò—Ç–µ—Ä–∞—Ü–∏—è –ø–æ map
for (const auto& [key, value] : my_map) {
    process(key, value);
}
```

#### std::optional –≤–º–µ—Å—Ç–æ —Å–ø–µ—Ü–∏–∞–ª—å–Ω—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π

**–•–û–†–û–®–û:**
```cpp
std::optional<int> find_element(const std::vector<int>& vec, int target) {
    auto it = std::find(vec.begin(), vec.end(), target);
    if (it != vec.end()) {
        return *it;
    }
    return std::nullopt;
}
```

#### –ü–∞—Ä–∞–ª–ª–µ–ª—å–Ω—ã–µ –∞–ª–≥–æ—Ä–∏—Ç–º—ã (C++17)

**–•–û–†–û–®–û:**
```cpp
#include <execution>

// –ü–∞—Ä–∞–ª–ª–µ–ª—å–Ω–∞—è —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞
std::sort(std::execution::par, vec.begin(), vec.end());

// –ü–∞—Ä–∞–ª–ª–µ–ª—å–Ω—ã–π –ø–æ–∏—Å–∫
auto it = std::find(std::execution::par, vec.begin(), vec.end(), value);
```

### –û–±—â–∏–µ –ø—Ä–∏–Ω—Ü–∏–ø—ã –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏

1. **–ò–∑–º–µ—Ä—è–π—Ç–µ, –Ω–µ –≥–∞–¥–∞–π—Ç–µ** - –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ –ø—Ä–æ—Ñ–∞–π–ª–µ—Ä—ã
2. **–û–ø—Ç–∏–º–∏–∑–∏—Ä—É–π—Ç–µ –∞–ª–≥–æ—Ä–∏—Ç–º, –Ω–µ –∫–æ–¥** - O(n log n) –≤—Å–µ–≥–¥–∞ –ª—É—á—à–µ O(n¬≤)
3. **–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –ø–æ–¥—Ö–æ–¥—è—â–∏–µ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã –¥–∞–Ω–Ω—ã—Ö** - –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –≤—ã–±–æ—Ä –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–∞ –∫—Ä–∏—Ç–∏—á–µ–Ω
4. **–ú–∏–Ω–∏–º–∏–∑–∏—Ä—É–π—Ç–µ –∫–æ–ø–∏—Ä–æ–≤–∞–Ω–∏—è** - –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ —Å—Å—ã–ª–∫–∏, move-—Å–µ–º–∞–Ω—Ç–∏–∫—É
5. **–£—á–∏—Ç—ã–≤–∞–π—Ç–µ –∫—ç—à –ø—Ä–æ—Ü–µ—Å—Å–æ—Ä–∞** - –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω—ã–π –¥–æ—Å—Ç—É–ø –∫ –ø–∞–º—è—Ç–∏ –±—ã—Å—Ç—Ä–µ–µ
6. **–ò–∑–±–µ–≥–∞–π—Ç–µ –ø—Ä–µ–∂–¥–µ–≤—Ä–µ–º–µ–Ω–Ω–æ–π –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏** - —Å–Ω–∞—á–∞–ª–∞ –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ—Å—Ç—å, –ø–æ—Ç–æ–º —Å–∫–æ—Ä–æ—Å—Ç—å
