–û—Ç–ª–∏—á–Ω–æ! –ù–µ —Ç–∞–∫ —Ö–æ—Ä–æ—à–æ, –∫–∞–∫ –ø—Ä–∏ –ø–µ—Ä–≤–æ–π –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏, –Ω–æ –≤—Å—ë-—Ç–∞–∫–∏ —É—Å–∫–æ—Ä–µ–Ω–∏–µ –ø–æ—á—Ç–∏ –≤ —Ç—Ä–∏ —Ä–∞–∑–∞ ‚Äî —ç—Ç–æ –æ—á–µ–Ω—å –Ω–µ–ø–ª–æ—Ö–æ.
–ü—Ä–æ–≤–µ—Ä—è–µ–º, –≤—Å—ë –ª–∏ —É—Å–∫–æ—Ä–∏–ª–∏
–í –ø—Ä–æ—à–ª–æ–º —É—Ä–æ–∫–µ –º—ã –ø—Ä–æ–¥–µ–ª–∞–ª–∏ –Ω–µ–ø–ª–æ—Ö—É—é —Ä–∞–±–æ—Ç—É: —É–ª—É—á—à–∏–ª–∏ —Å–∫–æ—Ä–æ—Å—Ç—å —Ä–∞–±–æ—Ç—ã –≤–Ω–∞—á–∞–ª–µ –≤ 50 —Ä–∞–∑, –∞ –∑–∞—Ç–µ–º –µ—â–µ –º–∏–Ω–∏–º—É–º –≤ –¥–≤–∞ —Ä–∞–∑–∞. –ö–∞–∑–∞–ª–æ—Å—å –±—ã, –º–µ—Ç–æ–¥—ã —É—Å–∫–æ—Ä–µ–Ω–∏—è –∏—Å—á–µ—Ä–ø–∞–Ω—ã. –û–¥–Ω–∞–∫–æ –ø–æ—Å–ª–µ–¥–Ω—é—é –æ–ø–µ—Ä–∞—Ü–∏—é ‚Äî –ø–æ–¥—Å—á—ë—Ç –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ –µ–¥–∏–Ω–∏—Ü ‚Äî –º—ã –ø–æ–∫–∞ –Ω–µ —Ç—Ä–æ–≥–∞–ª–∏. –•–æ—Ç—å –æ–Ω–∞ –∑–∞–Ω–∏–º–∞–µ—Ç –≤—Å–µ–≥–æ –æ–∫–æ–ª–æ 10% –≤—Ä–µ–º–µ–Ω–∏ –ø—Ä–æ–≥—Ä–∞–º–º—ã, –ø–æ—Ä–∞–±–æ—Ç–∞–µ–º –Ω–∞–¥ –Ω–µ–π —Ç–æ–∂–µ, –ø—Ä–æ—Å—Ç–æ —á—Ç–æ–±—ã –µ–π –Ω–µ –±—ã–ª–æ –æ–±–∏–¥–Ω–æ.
–í—Å–ø–æ–º–Ω–∏–º, —á—Ç–æ —É –Ω–∞—Å –µ—Å—Ç—å –∫ —ç—Ç–æ–º—É –º–æ–º–µ–Ω—Ç—É:
#include <algorithm>
#include <chrono>
#include <cstdlib>
#include <iostream>
#include <vector>

#include "log_duration.h"

using namespace std;

vector<int> ReverseVector3(const vector<int>& source_vector) {
    return {source_vector.rbegin(), source_vector.rend()};
}

int CountPops(const vector<int>& source_vector, int begin, int end) {
    int res = 0;

    for (int i = begin; i < end; ++i) {
        if (source_vector[i]) {
            ++res;
        }
    }

    return res;
}

void AppendRandom2(vector<int>& v, int n) {
    for (int i = 0; i < n; i += 15) {
        int number = rand();

        // –º—ã –º–æ–∂–µ–º –∑–∞–ø–æ–ª–Ω–∏—Ç—å 15 —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –≤–µ–∫—Ç–æ—Ä–∞,
        // –Ω–æ –Ω–µ –±–æ–ª–µ–µ, —á–µ–º –Ω–∞–º –æ—Å—Ç–∞–ª–æ—Å—å –¥–æ –∫–æ–Ω—Ü–∞:
        int count = min(15, n - i);

        for (int j = 0; j < count; ++j)
            // –æ–ø–µ—Ä–∞—Ü–∏—é —Å–¥–≤–∏–≥–∞ –±–∏—Ç–æ–≤ –≤—ã —É–∂–µ –≤–∏–¥–µ–ª–∏ –≤ —ç—Ç–æ–π –ø—Ä–æ–≥—Ä–∞–º–º–µ
            // –Ω–∞ —ç—Ç–æ—Ç —Ä–∞–∑ –¥–≤–∏–≥–∞–µ–º –≤–ø—Ä–∞–≤–æ, —á—Ç–æ–±—ã –Ω—É–∂–Ω—ã–π –±–∏—Ç –æ–∫–∞–∑–∞–ª—Å—è —Å–∞–º—ã–º –ø–æ—Å–ª–µ–¥–Ω–∏–º
            v.push_back((number >> j) % 2);
    }
}

void Operate() {
    LOG_DURATION("Total"s);
    vector<int> random_bits;
    // –û–ø–µ—Ä–∞—Ü–∏—è << –¥–ª—è —Ü–µ–ª—ã—Ö —á–∏—Å–µ–ª - —ç—Ç–æ —Å–¥–≤–∏–≥ –≤—Å–µ—Ö –±–∏—Ç –≤ –¥–≤–æ–∏—á–Ω–æ–π
    // –∑–∞–ø–∏—Å–∏ —á–∏—Å–ª–∞. –ó–∞–ø–∏—à–µ–º —Å –µ—ë –ø–æ–º–æ—â—å—é —á–∏—Å–ª–æ 2 –≤ —Å—Ç–µ–ø–µ–Ω–∏ 17 (131072).
    static const int N = 1 << 17;
    // –∑–∞–ø–æ–ª–Ω–∏–º –≤–µ–∫—Ç–æ—Ä —Å–ª—É—á–∞–π–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏ 0 –∏ 1
    {
        LOG_DURATION("Append random"s);
        AppendRandom2(random_bits, N);
    }
    // –ø–µ—Ä–µ–≤–µ—Ä–Ω—ë–º –≤–µ–∫—Ç–æ—Ä –∑–∞–¥–æ–º –Ω–∞–ø–µ—Ä—ë–¥
    vector<int> reversed_bits;
    {
        LOG_DURATION("Reverse"s);
        reversed_bits = ReverseVector3(random_bits);
    }

    {
        LOG_DURATION("Counting"s);
        // –ø–æ—Å—á–∏—Ç–∞–µ–º –ø—Ä–æ—Ü–µ–Ω—Ç –µ–¥–∏–Ω–∏—Ü –Ω–∞ –Ω–∞—á–∞–ª—å–Ω—ã—Ö –æ—Ç—Ä–µ–∑–∫–∞—Ö –≤–µ–∫—Ç–æ—Ä–∞
        for (int i = 1, step = 1; i <= N; i += step, step *= 2) {
            double rate = CountPops(reversed_bits, 0, i) * 100. / i;
            cout << "After "s << i << " digits we found "s << rate << "% pops"s << endl;
        }
    }
}
–ö–∞–∂–¥—ã–π —Ä–∞–∑ —Å—É–º–º–∏—Ä—É–µ–º –≤–µ—Å—å –¥–∏–∞–ø–∞–∑–æ–Ω, –∞ –º–æ–≥–ª–∏ –±—ã –ø—Ä–æ—Å—Ç–æ –¥–æ–±–∞–≤–ª—è—Ç—å –∫ —É–∂–µ —Ä–∞—Å—Å—á–∏—Ç–∞–Ω–Ω–æ–π —Å—É–º–º–µ –Ω–æ–≤—ã–µ —Å–ª–∞–≥–∞–µ–º—ã–µ. –î–ª—è —ç—Ç–æ–≥–æ –Ω—É–∂–Ω–æ —É—Å–ª–æ–∂–Ω–∏—Ç—å –∞–ª–≥–æ—Ä–∏—Ç–º:
{
    LOG_DURATION("Counting"s);
    // –ø–æ—Å—á–∏—Ç–∞–µ–º –ø—Ä–æ—Ü–µ–Ω—Ç –µ–¥–∏–Ω–∏—Ü –Ω–∞ –Ω–∞—á–∞–ª—å–Ω—ã—Ö –æ—Ç—Ä–µ–∑–∫–∞—Ö –≤–µ–∫—Ç–æ—Ä–∞
    int prev_sum = 0;
    int prev_i = 0;
    for (int i = 1, step = 1; i <= n; i += step, step *= 2) {
        const int sum = prev_sum + CountPops(reversed_bits, prev_i, i);

        cout << "After "s << i << " digits we found "s << (sum * 100. / i) << "% pops"s << endl;

        prev_i = i;
        prev_sum = sum;
    }
}
