vector<int> ComputeStatistics3(const vector<string>& measures, int n) {
    int m = measures.size();
    vector<int> result(n);

    for (int j = 0; j < m; ++j) {
        for (int i = 0; i < n; ++i) {
            // –±—É–¥–µ–º –ø—Ä–∏–±–∞–≤–ª—è—Ç—å —Ç–æ–ª—å–∫–æ –∫–æ–≥–¥–∞ –Ω–∞–¥–æ
            if (measures[j][i] >= 'K') {
                ++result[i];
            }
        }
    }

    return result;
}
