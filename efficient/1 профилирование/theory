–í–≥–ª—É–±—å –ø—Ä–æ—Ü–µ—Å—Å–æ—Ä–∞
–û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è ‚Äî —ç—Ç–æ –ø—Ä–æ—Ü–µ—Å—Å, –ø–æ–∑–≤–æ–ª—è—é—â–∏–π –∑–∞–º–µ–Ω–∏—Ç—å –æ–ø–µ—Ä–∞—Ü–∏–∏ –ø—Ä–æ–≥—Ä–∞–º–º—ã –Ω–∞ –±–æ–ª–µ–µ —ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω—ã–µ –∏ —Ç–µ–º —Å–∞–º—ã–º —É—Å–∫–æ—Ä–∏—Ç—å –≤—ã—á–∏—Å–ª–µ–Ω–∏—è –Ω–∞ –Ω–µ—Å–∫–æ–ª—å–∫–æ –ø—Ä–æ—Ü–µ–Ω—Ç–æ–≤ –∏–ª–∏ –≤ —Å–æ—Ç–Ω–∏ —Ä–∞–∑. –†–∞–∑–±–µ—Ä–µ–º—Å—è, –º–æ–∂–Ω–æ –ª–∏ –æ–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É, –Ω–µ –º–µ–Ω—è—è –æ–ø–µ—Ä–∞—Ü–∏–∏, –∫–æ—Ç–æ—Ä—ã–µ –æ–Ω–∞ —Å–æ–≤–µ—Ä—à–∞–µ—Ç.
–ü—Ä–µ–¥—Å—Ç–∞–≤–∏–º, —á—Ç–æ –≤ –Ω–µ–∫–æ—Ç–æ—Ä–æ–π –æ–±—Å–µ—Ä–≤–∞—Ç–æ—Ä–∏–∏ –µ—Å—Ç—å —Ç–µ–ª–µ—Å–∫–æ–ø. –ó–∞ –æ–¥–Ω—É –Ω–æ—á—å –æ–Ω —Å–æ–≤–µ—Ä—à–∞–µ—Ç –∏–∑–º–µ—Ä–µ–Ω–∏—è –Ω–∞ n —É—á–∞—Å—Ç–∫–∞—Ö –Ω–µ–±–∞. –†–µ–∑—É–ª—å—Ç–∞—Ç –∏–∑–º–µ—Ä–µ–Ω–∏—è –∫–æ–¥–∏—Ä—É–µ—Ç—Å—è –æ–¥–Ω–∏–º –ª–∞—Ç–∏–Ω—Å–∫–∏–º —Å–∏–º–≤–æ–ª–æ–º –æ—Ç A –¥–æ Z. –í —Ä–µ–∑—É–ª—å—Ç–∞—Ç–µ –ø–æ–ª—É—á–∞–µ—Ç—Å—è —Å—Ç—Ä–æ–∫–∞ –¥–ª–∏–Ω—ã n.

–°–æ–±—Ä–∞–Ω—ã —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã –∑–∞ –¥–ª–∏—Ç–µ–ª—å–Ω—ã–π –ø–µ—Ä–∏–æ–¥ —Ä–∞–±–æ—Ç—ã —Ç–µ–ª–µ—Å–∫–æ–ø–∞. –°—Ä–µ–¥–∏ –Ω–∏—Ö –Ω—É–∂–Ω–æ –Ω–∞–π—Ç–∏ —É—á–∞—Å—Ç–∫–∏ –Ω–µ–±–∞, –≥–¥–µ –±–æ–ª—å—à–µ –≤—Å–µ–≥–æ —Å–∏–ª—å–Ω—ã—Ö —Å–∏–≥–Ω–∞–ª–æ–≤ ‚Äî —Å –ª–∏—Ç–µ—Ä–æ–π K –∏–ª–∏ –¥–∞–ª–µ–µ. –†–µ–∞–ª–∏–∑—É–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø–æ–¥—Å—á—ë—Ç–∞ —Å–∏–ª—å–Ω—ã—Ö —Å–∏–≥–Ω–∞–ª–æ–≤ –ø–æ –∫–∞–∂–¥–æ–º—É —É—á–∞—Å—Ç–∫—É –Ω–µ–±–∞:
#include <string>
#include <vector>

using namespace std;

// –§—É–Ω–∫—Ü–∏—è –∞–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç –¥–∞–Ω–Ω—ã–µ —Ç–µ–ª–µ—Å–∫–æ–ø–∞, –æ–ø—Ä–µ–¥–µ–ª—è—è, —Å–∫–æ–ª—å–∫–æ —Å–∏–ª—å–Ω—ã—Ö —Å–∏–≥–Ω–∞–ª–æ–≤
// –∑–∞—Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–æ –ø–æ –∫–∞–∂–¥–æ–º—É –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—é.
vector<int> ComputeStatistics(const vector<string>& measures, int n) {
    int m = measures.size();
    vector<int> result(n);

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            if (measures[j][i] >= 'K') {
                ++result[i];
            }
        }
    }

    return result;
}
–§—É–Ω–∫—Ü–∏—è –≤–æ–∑–≤—Ä–∞—â–∞–µ—Ç –≤–µ–∫—Ç–æ—Ä, —ç–ª–µ–º–µ–Ω—Ç—ã –∫–æ—Ç–æ—Ä–æ–≥–æ ‚Äî —ç—Ç–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ —Å–∏–ª—å–Ω—ã—Ö —Å–∏–≥–Ω–∞–ª–æ–≤ –Ω–∞ –Ω—É–ª–µ–≤–æ–º —É—á–∞—Å—Ç–∫–µ –Ω–µ–±–∞, –ø–µ—Ä–≤–æ–º, –≤—Ç–æ—Ä–æ–º. –ü—Ä–æ—Ç–µ—Å—Ç–∏—Ä—É–µ–º —ç—Ç—É —Ñ—É–Ω–∫—Ü–∏—é, –∏–∑–º–µ—Ä–∏–≤ –≤—Ä–µ–º—è. –°—á–∏—Ç–∞–µ–º, —á—Ç–æ –ø—Ä–æ—à–ª–æ 5000 –¥–Ω–µ–π –∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —É—á–∞—Å—Ç–∫–æ–≤ –Ω–µ–±–∞ —Ç–æ–∂–µ —Ä–∞–≤–Ω–æ 5000. –†–µ–∞–ª—å–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö —Ç–µ–ª–µ—Å–∫–æ–ø–∞ —É –Ω–∞—Å –ø–æ–¥ —Ä—É–∫–æ–π –Ω–µ—Ç, –ø–æ—ç—Ç–æ–º—É –∑–∞–ø–æ–ª–Ω–∏–º –≤–µ–∫—Ç–æ—Ä —Å–ª—É—á–∞–π–Ω—ã–º–∏ –±—É–∫–≤–∞–º–∏:
#include "log_duration.h"

#include <algorithm>
#include <iostream>
#include <numeric>
#include <random>
#include <string>
#include <vector>

using namespace std;

// —Ñ—É–Ω–∫—Ü–∏—è –∞–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç –¥–∞–Ω–Ω—ã–µ —Ç–µ–ª–µ—Å–∫–æ–ø–∞, –æ–ø—Ä–µ–¥–µ–ª—è—è, —Å–∫–æ–ª—å–∫–æ —Å–∏–ª—å–Ω—ã—Ö —Å–∏–≥–Ω–∞–ª–æ–≤
// –∑–∞—Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–æ –ø–æ –∫–∞–∂–¥–æ–º—É –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—é
vector<int> ComputeStatistics(const vector<string>& measures, int n) {
    int m = measures.size();
    vector<int> result(n);

    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            // –ø—Ä–∏–±–∞–≤–ª—è–µ–º 1, –µ—Å–ª–∏ —Å–∏–≥–Ω–∞–ª –Ω–µ –º–µ–Ω–µ–µ 'K'
            result[i] += (measures[j][i] >= 'K' ? 1 : 0);
        }
    }

    return result;
}

string GetRandomString(int size) {
    static mt19937 engine;
    uniform_int_distribution<int> distribution('A', 'Z');

    string res(size, ' ');
    for (char& c : res) {
        c = char(distribution(engine));
    }

    return res;
}

int main() {
    vector<string> data;

    for (int i = 0; i < 5000; ++i) {
        data.push_back(GetRandomString(5000));
    }

    vector<int> statistics;
    {
        LOG_DURATION("ComputeStatistics"s);
        statistics = ComputeStatistics(data, 5000);
    }

    cout << "–í—Å–µ–≥–æ —Å–∏–ª—å–Ω—ã—Ö —Å–∏–≥–Ω–∞–ª–æ–≤: "s << accumulate(statistics.begin(), statistics.end(), 0) << endl;
}
